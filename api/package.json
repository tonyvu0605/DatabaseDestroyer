    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "luxon": "^3.4.4",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.4",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
