    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-security-node": "^1.1.4",
    "eslint-plugin-sql": "^2.5.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
